{
	"name": "neosense/core-bundle",
	"type": "symfony-bundle",
	"description": "NS Core Bundle",
	"license": "Proprietary",
	"authors": [
		{
			"name": "Neosense"
		}
	],
	"minimum-stability": "dev",
	"require": {
		"php":                                  ">=5.3.3",
		"symfony/symfony":                      "2.3.*@stable",
		"doctrine/orm":                         ">=2.2.3,<2.4-dev@stable",
		"doctrine/doctrine-bundle":             "1.2.*@stable",
        "twig/extensions":                      "1.0.*@stable",
        "symfony/assetic-bundle":               "2.3.*@stable",
        "symfony/swiftmailer-bundle":           "2.3.2@stable",
        "symfony/monolog-bundle":               "2.3.*@stable",
        "sensio/distribution-bundle":           "2.3.*@stable",
        "sensio/framework-extra-bundle":        "2.3.*@stable",
        "sensio/generator-bundle":              "2.3.*@stable",
		"incenteev/composer-parameter-handler": "~2.0@stable",

		"avalanche123/imagine-bundle":          "2.1.*@stable",
		"doctrine/doctrine-migrations-bundle":  "dev-master@dev",
		"doctrine/migrations":                  "dev-master@dev",
		"excelwebzone/zend-search":             "dev-master@dev",
		"friendsofsymfony/user-bundle":         "1.3.*@stable",
		"jms/security-extra-bundle":            "1.5.*@stable",
		"jms/di-extra-bundle":                  "1.4.*@stable",
		"knplabs/knp-menu-bundle":              "2.0.*@alpha",
		"knplabs/knp-menu":                     "2.0.*@alpha",
		"knplabs/knp-paginator-bundle":         "2.3.*@stable",
		"liuggio/excelbundle":                  "1.0.*@stable",
		"mopa/bootstrap-bundle":                "2.2.*@alpha",
		"stfalcon/tinymce-bundle":              "0.2.*@stable",
		"stof/doctrine-extensions-bundle":      "1.1.*@stable",
		"symfony/dom-crawler":                  "2.3.*@stable",
		"twbs/bootstrap":                       "2.3.*@stable",
		
		"neosense/admin-bundle":                "1.2.*@stable",
		"neosense/catalog-bundle":              "1.4.*@stable",
		"neosense/cms-bundle":                  "1.2.*@stable",
		"neosense/deploy-bundle":               "1.1.*@stable",
		"neosense/documents-bundle":            "1.1.*@stable",
		"neosense/feedback-bundle":             "1.0.*@stable",
		"neosense/gallery-bundle":              "1.0.*@stable",
		"neosense/news-bundle":                 "1.1.*@stable",
		"neosense/pages-bundle":                "1.1.*@stable",
		"neosense/properties-bundle":           "1.0.*@stable",
		"neosense/routing-bundle":              "1.0.*@stable",
		"neosense/search-bundle":               "1.1.*@stable",
		"neosense/shop-bundle":                 "1.1.*@stable",
		"neosense/user-bundle":                 "1.1.*@stable"
	},
	"scripts": {
        "post-install-cmd": [
			"Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
			"Mopa\\Bundle\\BootstrapBundle\\Composer\\ScriptHandler::postInstallSymlinkTwitterBootstrap"
        ],
        "post-update-cmd": [
			"Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
			"Mopa\\Bundle\\BootstrapBundle\\Composer\\ScriptHandler::postInstallSymlinkTwitterBootstrap"
        ]
    },
	"autoload": {
		"psr-0": { "NS\\CoreBundle": "" }
	},
	"target-dir": "NS/CoreBundle"
}